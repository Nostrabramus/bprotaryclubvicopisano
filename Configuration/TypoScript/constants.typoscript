############
### PAGE ###
############
page {
    logo {
        file = EXT:bprotaryclubvicopisano/Resources/Public/Images/logo-rc-vicopisano.png
        fileInverted = EXT:bprotaryclubvicopisano/Resources/Public/Images/logo-rc-vicopisano.png
        height = 62
        width = 180
        alt = BPRotaryClubVicopisano
        linktitle = BPRotaryClubVicopisano
    }
    favicon {
        file = EXT:bprotaryclubvicopisano/Resources/Public/Icons/favicon.ico
    }
    fluidtemplate {
        layoutRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Layouts/Page/
        partialRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Partials/Page/
        templateRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Templates/Page/
    }
}


##################
### EXTENSIONS ###
##################
plugin.bootstrap_package {
    view {
        layoutRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Layouts/
        partialRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Partials/
        templateRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Templates/
    }
}


########################
### CONTENT ELEMENTS ###
########################
plugin.bootstrap_package_contentelements {
    view {
        layoutRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Layouts/ContentElements/
        partialRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Partials/ContentElements/
        templateRootPath = EXT:bprotaryclubvicopisano/Resources/Private/Templates/ContentElements/
    }
}


